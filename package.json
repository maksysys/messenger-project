{
    "name": "my-project",
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "build": "vite build",
        "dev": "vite"
    },
    "dependencies": {
        "@fontsource/inter": "^5.0.21",
        "@types/node": "^22.7.9",
        "express": "^4.19.2",
        "snabbdom": "^3.6.2",
        "snabbdom-jsx-lite": "^2.0.1",
        "vite": "^5.4.3"
    },
    "devDependencies": {
        "@types/express": "^5.0.0",
        "typescript": "^5.6.3"
    },
    "compilerOptions": {
        "jsx": "react",
        "jsxFactory": "jsx",
        "target": "es2022",
        "module": "es2022",
        "moduleResolution": "Node",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "strict": true,
        "lib": [
            "ES2020",
            "DOM"
        ]
    },
    "include": [
        "src/**/*.ts",
        "src/**/*.tsx"
    ],
    "exclude": [
        "node_modules"
    ]
}
